         <property name="styleSheet">
          <string notr="true">/* 设置整个日历的背景颜色 */
QCalendarWidget {
    background-color: #f0f0f0;
    color: #333333;
    border: 1px solid #dcdcdc;
}

/* 设置导航栏的样式 */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #4a90e2;
    color: white;
}

/* 设置前一个月按钮的背景图片 */
QCalendarWidget QToolButton#qt_calendar_prevmonth {
	qproperty-icon: url(:/tool/tool/left.png);
    background-color: transparent; /* 透明背景 */
    border: none;
    border-radius: 4px;
    padding: 5px;
    min-width: 30px;
    min-height: 30px;
    icon-size: 16px;
}

/* 设置下一个月按钮的背景图片 */
QCalendarWidget QToolButton#qt_calendar_nextmonth {
	qproperty-icon: url(:/tool/tool/right.png);
    background-color: transparent; /* 透明背景 */
    border: none;
    border-radius: 4px;
    padding: 5px;
    min-width: 30px;
    min-height: 30px;
    icon-size: 16px;
}

/* 设置前一个月按钮的样式 */
QCalendarWidget QToolButton#qt_calendar_prevmonth {
    background-color: #4a90e2;
    color: white;
    border: none;
    border-radius: 4px;
    padding: 5px;
    min-width: 30px;
    min-height: 30px;
    icon-size: 16px;
